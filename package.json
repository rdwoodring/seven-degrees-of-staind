{
    "name": "seven-degrees-of-staind",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "test": "node scripts/test.js",
        "start": "node ./bin/www",
        "build": "npm run build-server-ts && cd client && npm run build && rm ../public/static -rvf && mv build/static ../public/static && mv build/* ../public",
        "build-server-ts": "tsc",
        "watch-node": "nodemon dist ./bin/www",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-client\"",
        "watch-ts": "tsc -w",
        "watch-client": "cd client && npm run watch"
    },
    "dependencies": {
        "axios": "0.21.1",
        "body-parser": "1.19.0",
        "connect-mongo": "3.0.0",
        "cookie-parser": "1.3.5",
        "debug": "4.1.0",
        "dotenv": "4.0.0",
        "express": "4.16.4",
        "express-session": "1.16.2",
        "lodash": "4.17.14",
        "mongoose": "5.8.11",
        "morgan": "1.9.1",
        "p-queue": "6.0.2",
        "query-string": "6.8.1",
        "serve-favicon": "2.5.0"
    },
    "devDependencies": {
        "@types/body-parser": "1.17.0",
        "@types/connect-mongo": "0.0.42",
        "@types/cookie-parser": "1.4.1",
        "@types/debug": "4.1.4",
        "@types/dotenv": "6.1.1",
        "@types/express": "4.16.1",
        "@types/express-session": "1.15.13",
        "@types/jest": "24.0.18",
        "@types/lodash": "4.14.125",
        "@types/mongoose": "5.7.0",
        "@types/morgan": "1.7.35",
        "@types/query-string": "6.3.0",
        "concurrently": "4.1.0",
        "jest": "24.9.0",
        "mongodb": "3.2.7",
        "nodemon": "1.19.1",
        "ts-jest": "24.1.0",
        "typescript": "3.4.5"
    }
}
