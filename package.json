{
    "name": "seven-degrees-of-staind",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "build-server-ts": "tsc",
        "watch-node": "nodemon dist ./bin/www",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-client\"",
        "watch-ts": "tsc -w",
        "watch-client": "cd client && npm run watch"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "~1.3.5",
        "debug": "^4.1.0",
        "dotenv": "^4.0.0",
        "express": "^4.16.4",
        "lodash": "^4.17.11",
        "morgan": "^1.9.1",
        "querystring": "^0.2.0",
        "request": "^2.88.0",
        "serve-favicon": "^2.5.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/cookie-parser": "^1.4.1",
        "@types/debug": "^4.1.4",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.16.1",
        "@types/lodash": "^4.14.125",
        "@types/morgan": "^1.7.35",
        "@types/request": "^2.48.1",
        "concurrently": "^4.1.0",
        "nodemon": "^1.19.1",
        "typescript": "^3.4.5"
    }
}
